<h1 class='page-header'>All Bands:</h1>

<ul class='main-list'>
  <% @bands.each do |band| %>
    <li><a href="<%= band_url(band) %>"><p><%= band.name %></p></a></li>
  <% end %>
</ul>

<ul class='page-links'>
  <li>
    <a class='button' href="<%= new_band_url %>" >Create new band</a>
  </li>
</ul>
